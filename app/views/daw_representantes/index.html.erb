<h1>Listing Daw Representantes</h1>

<table>
  <thead>
    <tr>
      <th>Repr profesion</th>
      <th>Repr sostenfamiliar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_representantes.each do |daw_representante| %>
      <tr>
        <td><%= daw_representante.repr_profesion %></td>
        <td><%= daw_representante.repr_sostenfamiliar %></td>
        <td><%= link_to 'Show', daw_representante %></td>
        <td><%= link_to 'Edit', edit_daw_representante_path(daw_representante) %></td>
        <td><%= link_to 'Destroy', daw_representante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<p id="notice"><%= notice %></p>

<h1>Listing Daw Curso Promos</h1>

<table>
  <thead>
    <tr>
      <th>Daw promo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_curso_promos.each do |daw_curso_promo| %>
      <tr>
        <td><%= daw_curso_promo.daw_promo %></td>
        <td><%= link_to 'Show', daw_curso_promo %></td>
        <td><%= link_to 'Edit', edit_daw_curso_promo_path(daw_curso_promo) %></td>
        <td><%= link_to 'Destroy', daw_curso_promo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


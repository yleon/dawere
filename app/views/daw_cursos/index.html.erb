<p id="notice"><%= notice %></p>

<h1>Listing Daw Cursos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_cursos.each do |daw_curso| %>
      <tr>
        <td><%= link_to 'Show', daw_curso %></td>
        <td><%= link_to 'Edit', edit_daw_curso_path(daw_curso) %></td>
        <td><%= link_to 'Destroy', daw_curso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<p id="notice"><%= notice %></p>

<h1>Listing Daw Actividads</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_actividads.each do |daw_actividad| %>
      <tr>
        <td><%= link_to 'Show', daw_actividad %></td>
        <td><%= link_to 'Edit', edit_daw_actividad_path(daw_actividad) %></td>
        <td><%= link_to 'Destroy', daw_actividad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daw actividad', new_daw_actividad_path %>

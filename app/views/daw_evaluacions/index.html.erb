<p id="notice"><%= notice %></p>

<h1>Listing Daw Evaluacions</h1>

<table>
  <thead>
    <tr>
      <th>Ponderacion</th>
      <th>Periodo</th>
      <th>Orden</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_evaluacions.each do |daw_evaluacion| %>
      <tr>
        <td><%= daw_evaluacion.eval_ponderacion %></td>
        <td><%= daw_evaluacion.eval_periodo %></td>
        <td><%= daw_evaluacion.eval_orden %></td>
        <td><%= daw_evaluacion.eval_tipoeval %></td>
        <td><%= link_to 'Show', daw_evaluacion %></td>
        <td><%= link_to 'Edit', edit_daw_evaluacion_path(daw_evaluacion) %></td>
        <td><%= button_to 'Destroy', daw_evaluacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daw evaluacion', new_daw_evaluacion_path %>

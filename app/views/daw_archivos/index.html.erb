<p id="notice"><%= notice %></p>

<h1>Listing Daw Archivos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_archivos.each do |daw_archivo| %>
      <tr>
        <td><%= link_to 'Show', daw_archivo %></td>
        <td><%= link_to 'Edit', edit_daw_archivo_path(daw_archivo) %></td>
        <td><%= link_to 'Destroy', daw_archivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<p id="notice"><%= notice %></p>

<h1>Listing Daw Trabajos</h1>

<table>
  <thead>
    <tr>
      <th>Trab nombre</th>
      <th>Trab tipo</th>
      <th>Trab telefono</th>
      <th>Trab direccion</th>
      <th>Trab fecha</th>
      <th>Trab estado</th>
      <th>Daw persona</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_trabajos.each do |daw_trabajo| %>
      <tr>
        <td><%= daw_trabajo.trab_nombre %></td>
        <td><%= daw_trabajo.trab_tipo %></td>
        <td><%= daw_trabajo.trab_telefono %></td>
        <td><%= daw_trabajo.trab_direccion %></td>
        <td><%= daw_trabajo.trab_fecha %></td>
        <td><%= daw_trabajo.trab_estado %></td>
        <td><%= daw_trabajo.daw_persona %></td>
        <td><%= link_to 'Show', daw_trabajo %></td>
        <td><%= link_to 'Edit', edit_daw_trabajo_path(daw_trabajo) %></td>
        <td><%= link_to 'Destroy', daw_trabajo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


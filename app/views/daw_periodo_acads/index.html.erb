<p id="notice"><%= notice %></p>

<h1>Listing Daw Periodo Acads</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Fecha de inicio</th>
      <th>Fecha de culminacion</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daw_periodo_acads.each do |daw_periodo_acad| %>
      <tr>
        <td><%= daw_periodo_acad.pac_nombre %></td>
        <td><%= daw_periodo_acad.pac_descripcion %></td>
        <td><%= daw_periodo_acad.pac_fechaini %></td>
        <td><%= daw_periodo_acad.pac_fechafin %></td>
        <td><%= daw_periodo_acad.pac_estado %></td>
        <td><%= link_to 'Show', daw_periodo_acad %></td>
        <td><%= link_to 'Edit', edit_daw_periodo_acad_path(daw_periodo_acad) %></td>
        <td><%= button_to 'Destroy', daw_periodo_acad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daw periodo acad', new_daw_periodo_acad_path %>